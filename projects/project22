var day = 2

switch (day) {
    case 1:
        console.log("monday")
        break;

    case 2 :
        console.log("tuesday")
        break;

     case 3:
        console.log("wendnesday")
        break;
    default:
        console.log("please, provide day")
}

var c = 13.8

console.log(c.toFloor())
console.log(c.toceil())
